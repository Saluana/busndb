{
    "name": "busndb",
    "version": "0.1.0",
    "description": "A developer-friendly, embeddable NoSQL database layer on top of SQLite with full TypeScript support",
    "main": "index.ts",
    "module": "index.ts",
    "type": "module",
    "keywords": [
        "database",
        "sqlite",
        "nosql",
        "typescript",
        "zod",
        "embeddable",
        "bun"
    ],
    "author": "Brendon",
    "license": "MIT",
    "dependencies": {
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "bun-types": "latest"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "scripts": {
        "test": "bun test",
        "dev": "bun run index.ts",
        "example": "bun run example.ts",
        "benchmark": "bun run benchmark.ts",
        "benchmark:sql": "bun run benchmark-sql-translator.ts"
    },
    "files": [
        "src/**/*",
        "index.ts",
        "README.md"
    ]
}
